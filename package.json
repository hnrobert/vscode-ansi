{
  "name": "vscode-ansi",
  "publisher": "HNRobert",
  "displayName": "ANSI Previewer",
  "description": "ANSI color styling and previewer for text documents",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/HNRobert/vscode-ansi"
  },
  "bugs": {
    "url": "https://github.com/HNRobert/vscode-ansi/issues",
    "email": "hnrobert@qq.com"
  },
  "packageManager": "pnpm@10.13.1",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Themes",
    "Visualization"
  ],
  "keywords": [
    "ansi",
    "escapes",
    "colors",
    "logs"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "HNRobert.vscode-ansi.showPretty",
        "title": "Open Preview",
        "category": "ANSI Text",
        "icon": "$(open-preview)"
      },
      {
        "command": "HNRobert.vscode-ansi.showPrettyToSide",
        "title": "Open Preview to the Side",
        "category": "ANSI Text",
        "icon": "$(open-preview)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "HNRobert.vscode-ansi.showPrettyToSide",
          "alt": "HNRobert.vscode-ansi.showPretty",
          "when": "editorLangId == ansi",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "HNRobert.vscode-ansi.showPretty",
        "key": "shift+ctrl+v",
        "mac": "shift+cmd+v",
        "when": "editorLangId == ansi"
      },
      {
        "command": "HNRobert.vscode-ansi.showPrettyToSide",
        "key": "ctrl+k v",
        "mac": "cmd+k v",
        "when": "editorLangId == ansi"
      }
    ],
    "languages": [
      {
        "id": "ansi",
        "aliases": [
          "ANSI Text"
        ],
        "extensions": [
          ".ans",
          ".ansi"
        ],
        "configuration": "./ansi.language-configuration.json"
      }
    ]
  },
  "scripts": {
    "build": "tsup",
    "vscode:prepublish": "pnpm run build",
    "lint": "eslint src --ext ts",
    "watch": "tsup --watch",
    "pretest": "pnpm run build && pnpm run lint",
    "test": "node ./out/test/runTest.js",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vscode/vsce": "^2.18.0",
    "eslint": "^8.35.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "prettier": "^2.8.4",
    "tsup": "^8.0.1",
    "typescript": "~5.1.6",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {}
}
